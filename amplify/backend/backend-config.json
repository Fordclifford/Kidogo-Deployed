{
	"auth": {
		"Kidogo": {
			"service": "Cognito",
			"providerPlugin": "awscloudformation"
		}
	},
	"storage": {
		"KidogoDB": {
			"service": "DynamoDB",
			"providerPlugin": "awscloudformation"
		}
	},
	"function": {
		"KidogoLambda": {
			"service": "Lambda",
			"providerPlugin": "awscloudformation",
			"build": true,
			"dependsOn": [
				{
					"category": "storage",
					"resourceName": "KidogoDB",
					"attributes": [
						"Name",
						"Arn"
					]
				}
			]
		}
	},
	"api": {
		"KidogoApi": {
			"service": "API Gateway",
			"providerPlugin": "awscloudformation",
			"dependsOn": [
				{
					"category": "function",
					"resourceName": "KidogoLambda",
					"attributes": [
						"Name",
						"Arn"
					]
				}
			]
		}
	}
}